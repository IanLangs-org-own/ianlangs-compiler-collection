#std io

template <typename T = int>
tagged union[2] result<T, void> {Ok, Err}

using namespace flow;

fn main() -> int {
    result<int> r;

    r = result<int>::Err;

    switch (r) {
        case result<>::Ok:
            println("Ok = ", r.value.Ok);
            break;
        case result<>::Err:
            println("Err");
            break;
    }

    return 0;
}